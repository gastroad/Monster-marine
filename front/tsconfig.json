{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "dist/",
    "target": "esnext",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@containers/*": ["./src/containers/*"],
      "@src/*": ["./src/*"]
    }
  }
}
